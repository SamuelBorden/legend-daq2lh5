{
  "outputs": ["presummed_waveform"],
  "processors": {
    "windowed_waveform": {
      "start_index": 1000,
      "end_index": -1000
    },
    "presummed_waveform": {
      "function": "presum",
      "module": "pygama.dsp.processors",
      "args": ["waveform", "presummed_waveform(len(waveform)/4, 'f')"],
      "unit": "ADC"
    }
  }
}
